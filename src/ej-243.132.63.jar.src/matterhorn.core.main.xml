<idea-plugin package="com.intellij.ml.llm.matterhorn">

  <dependencies>
    <module name="intellij.platform.vcs.impl"/>
    <module name="intellij.platform.collaborationTools"/>
    <module name="intellij.platform.vcs.log.impl"/>
  </dependencies>

  <extensions defaultExtensionNs="com.intellij">
    <registryKey defaultValue="false" description="Force file cache" key="matterhorn.force.file.cache"/>
  </extensions>
</idea-plugin>
